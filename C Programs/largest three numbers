#include <stdio.h>
int main()
{
    int a,y,s;
    printf("enter a,enter y,enter s:");
    scanf("%d %d %d",&a,&y,&s);
    if(a>y)
    {
        printf("a is larger");
    }
    else if(y>s)
    {
        printf("y is larger");
    }
    else if(s>a)
    {
        printf("s is larger");  
    }
    else
    {
        printf("both are equal"); 
    }
}

