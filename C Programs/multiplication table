#include <stdio.h>
int main()
{
    int x,y,z,a;
    printf("enter y and enter a");
    scanf("%d %d",&y,&a);
    z=1;
    for(x=1;x<=a;x++)
    {
        z=y*x;
        printf(" %d*%d= %d\n",x,y,z);
    }
        return 0;
}


